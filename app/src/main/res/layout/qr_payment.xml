<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">


        <TextView
            android:id="@+id/account5"
            android:layout_width="52dp"
            android:layout_height="21dp"
            android:layout_marginTop="4dp"
            android:layout_marginEnd="28dp"
            android:text="Account"
            android:textColor="@color/black"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/accountLogo6" />

        <TextView
            android:id="@+id/history5"
            android:layout_width="45dp"
            android:layout_height="22dp"
            android:layout_marginTop="4dp"
            android:layout_marginEnd="264dp"
            android:text="History"
            android:textColor="@color/black"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/historyLogo5" />

        <TextView
            android:id="@+id/status5"
            android:layout_width="47dp"
            android:layout_height="23dp"
            android:layout_marginTop="4dp"
            android:layout_marginEnd="100dp"
            android:text="Status"
            android:textColor="@color/black"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/statusLogo5" />

        <TextView
            android:id="@+id/booking5"
            android:layout_width="52dp"
            android:layout_height="21dp"
            android:layout_marginTop="4dp"
            android:layout_marginEnd="180dp"
            android:text="Booking"
            android:textColor="@color/black"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/bookingLogo5" />

        <TextView
            android:id="@+id/home5"
            android:layout_width="42dp"
            android:layout_height="20dp"
            android:layout_marginStart="24dp"
            android:layout_marginTop="4dp"
            android:text="Home"
            android:textColor="@color/black"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/primeLaundryLogoHome5" />

        <ImageView
            android:id="@+id/accountLogo6"
            android:layout_width="55dp"
            android:layout_height="55dp"
            android:layout_centerHorizontal="true"
            android:layout_marginStart="16dp"
            android:layout_marginTop="644dp"
            android:layout_marginEnd="16dp"
            android:background="@drawable/account"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/statusLogo5"
            app:layout_constraintTop_toTopOf="parent" />

        <ImageView
            android:id="@+id/statusLogo5"
            android:layout_width="55dp"
            android:layout_height="55dp"
            android:layout_centerHorizontal="true"
            android:layout_marginStart="20dp"
            android:layout_marginTop="644dp"
            android:background="@drawable/status"
            app:layout_constraintStart_toEndOf="@+id/bookingLogo5"
            app:layout_constraintTop_toTopOf="parent" />

        <ImageView
            android:id="@+id/historyLogo5"
            android:layout_width="55dp"
            android:layout_height="55dp"
            android:layout_centerHorizontal="true"
            android:layout_marginStart="16dp"
            android:layout_marginTop="644dp"
            android:layout_marginEnd="16dp"
            android:background="@drawable/history"
            app:layout_constraintEnd_toStartOf="@+id/bookingLogo5"
            app:layout_constraintStart_toEndOf="@+id/primeLaundryLogoHome5"
            app:layout_constraintTop_toTopOf="parent" />

        <ImageView
            android:id="@+id/bookingLogo5"
            android:layout_width="55dp"
            android:layout_height="55dp"
            android:layout_centerHorizontal="true"
            android:layout_marginStart="148dp"
            android:layout_marginTop="644dp"
            android:layout_marginEnd="148dp"
            android:background="@drawable/booking"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <ImageView
            android:id="@+id/primeLaundryLogoHome5"
            android:layout_width="55dp"
            android:layout_height="55dp"
            android:layout_centerHorizontal="true"
            android:layout_marginStart="16dp"
            android:layout_marginTop="644dp"
            android:background="@drawable/logo"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <!--    <androidx.cardview.widget.CardView-->
        <!--        android:id="@+id/cardViewDryCleaning"-->
        <!--        android:layout_width="295dp"-->
        <!--        android:layout_height="375dp"-->
        <!--        android:layout_margin="20dp"-->
        <!--        app:cardBackgroundColor="#FFFFFF"-->
        <!--        app:cardCornerRadius="30dp"-->
        <!--        app:cardElevation="5dp"-->
        <!--        app:layout_constraintEnd_toEndOf="parent"-->
        <!--        app:layout_constraintStart_toStartOf="parent"-->
        <!--        app:layout_constraintTop_toBottomOf="@+id/payment">-->

        <!--    </androidx.cardview.widget.CardView>-->

        <ImageView
            android:id="@+id/foldImg"
            android:layout_width="42dp"
            android:layout_height="46dp"
            android:layout_centerHorizontal="true"
            android:layout_marginStart="65dp"
            android:layout_marginTop="10dp"
            android:background="@drawable/summary"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/payment" />

        <TextView
            android:id="@+id/qrPayment"
            android:layout_width="149dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="11dp"
            android:layout_marginTop="22dp"
            android:layout_marginEnd="144dp"
            android:text="DR PAYMENT"
            android:textSize="15dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/foldImg"
            app:layout_constraintTop_toBottomOf="@+id/payment" />


        <TextView
            android:id="@+id/payment"
            android:layout_width="278dp"
            android:layout_height="43dp"
            android:layout_marginTop="16dp"
            android:text="Payment"
            android:textColor="@color/black"
            android:textSize="25dp"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.496"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textAppsName4" />

        <TextView
            android:id="@+id/textAppsName4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="100dp"
            android:layout_marginTop="45dp"
            android:layout_marginEnd="101dp"
            android:fontFamily="@font/abril_fatface"
            android:text="Prime Laundry"
            android:textSize="30dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <Button
            android:id="@+id/uploadReceipt"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="100dp"
            android:layout_marginEnd="100dp"
            android:layout_marginBottom="84dp"
            android:text="UPLOAD RECEIPT"
            app:layout_constraintBottom_toTopOf="@+id/statusLogo5"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.489"
            app:layout_constraintStart_toStartOf="parent" />


